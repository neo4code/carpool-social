{
  "builds": [
    {
      "src": "ui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "cd ui && pnpm install && pnpm build",
  "outputDirectory": "ui/dist",
  "installCommand": "pnpm install",
  "framework": "vite"
}
